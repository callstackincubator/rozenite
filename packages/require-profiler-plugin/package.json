{
  "name": "@rozenite/require-profiler-plugin",
  "version": "1.2.0",
  "description": "Metro require profiler for Rozenite.",
  "type": "module",
  "main": "./dist/react-native.cjs",
  "module": "./dist/react-native.js",
  "types": "./dist/react-native.d.ts",
  "exports": {
    ".": {
      "types": "./dist/react-native.d.ts",
      "import": "./dist/react-native.js",
      "require": "./dist/react-native.cjs"
    },
    "./metro": {
      "types": "./dist/metro.d.ts",
      "import": "./dist/metro.js",
      "require": "./dist/metro.cjs"
    }
  },
  "scripts": {
    "build": "rozenite build",
    "dev": "rozenite dev",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@rozenite/plugin-bridge": "workspace:*",
    "@rozenite/tools": "workspace:*",
    "nanoevents": "^9.1.0"
  },
  "devDependencies": {
    "@rozenite/vite-plugin": "workspace:*",
    "@types/react": "~18.3.12",
    "autoprefixer": "^10.4.21",
    "lucide-react": "^0.263.1",
    "metro-config": "*",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.0",
    "react-flame-graph": "^1.4.0",
    "react-json-tree": "^0.20.0",
    "react-native": "0.76.0",
    "react-native-web": "0.21.0",
    "rozenite": "workspace:*",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "license": "MIT"
}
